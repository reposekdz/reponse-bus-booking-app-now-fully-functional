{
  "name": "gobus",
  "private": true,
  "version": "1.0.0",
  "description": "A sleek, modern, and highly intuitive bus booking platform named GoBus, for seamless journeys across Rwanda, featuring a premium design inspired by the nation's vibrant colors.",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "analyze": "npx vite-bundle-analyzer",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@google/genai": "^0.14.0",
    "axios": "^1.6.2",
    "html2canvas": "^1.4.1",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "socket.io-client": "^4.7.5"
  },
  "devDependencies": {
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "terser": "^5.44.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.10"
  }
}
