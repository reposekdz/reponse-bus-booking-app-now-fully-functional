# Vite Environment Variables (must be prefixed with VITE_)

# API Configuration
VITE_API_URL=http://localhost:5000
VITE_WS_URL=ws://localhost:5000

# Google AI Configuration
GEMINI_API_KEY=your-gemini-api-key-here

# App Configuration
VITE_APP_NAME=GoBus
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Modern Bus Booking Platform

# External Services
VITE_GOOGLE_MAPS_API_KEY=your-google-maps-api-key
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key

# Firebase Configuration (for web push notifications)
VITE_FIREBASE_API_KEY=your-firebase-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-firebase-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdef123456
VITE_FIREBASE_VAPID_KEY=your-vapid-key

# Feature Flags
VITE_ENABLE_PWA=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_CHAT_SUPPORT=true
VITE_ENABLE_LOYALTY_PROGRAM=true
VITE_ENABLE_REAL_TIME_TRACKING=true

# Development Configuration
VITE_ENABLE_DEVTOOLS=true
VITE_ENABLE_MOCK_DATA=false
VITE_DEBUG_MODE=false

# Social Media Links
VITE_FACEBOOK_URL=https://facebook.com/gobus
VITE_TWITTER_URL=https://twitter.com/gobus
VITE_INSTAGRAM_URL=https://instagram.com/gobus
VITE_LINKEDIN_URL=https://linkedin.com/company/gobus

# Contact Information
VITE_SUPPORT_EMAIL=support@gobus.com
VITE_SUPPORT_PHONE=+250123456789
VITE_COMPANY_ADDRESS=Kigali, Rwanda